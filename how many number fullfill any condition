    cin >> n;

    int count = 0; // Counter to keep track of the numbers fulfilling the condition

    cout << "Enter " << n << " numbers:" << endl;
    for (int i = 0; i < n; i++) {
        int num;
        cin >> num;

        // Check the condition
        if (/* Your condition here */) {
            count++;
        }
    }

    cout << "Number of numbers fulfilling the condition: " << count << endl;

    return 0;
}
